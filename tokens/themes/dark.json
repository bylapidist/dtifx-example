{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "$version": "1.0.0",
  "$description": "Dark theme overrides with accessible text contrast.",
  "$overrides": [
    {
      "$token": "#/cmp/btn/bg",
      "$when": {
        "theme": "dark"
      },
      "$value": {
        "colorSpace": "srgb",
        "components": [
          0.8392156862745098,
          0.8705882352941177,
          0.9254901960784314
        ]
      }
    },
    {
      "$token": "#/cmp/btn/fg",
      "$when": {
        "theme": "dark"
      },
      "$value": {
        "colorSpace": "srgb",
        "components": [
          0.058823529411764705,
          0.09411764705882353,
          0.14901960784313725
        ]
      }
    }
  ]
}
