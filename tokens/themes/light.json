{
  "$schema": "https://dtif.lapidist.net/schema/core.json",
  "$version": "1.0.0",
  "$description": "Light theme overrides with high-contrast accessibility adjustments.",
  "$overrides": [
    {
      "$token": "#/cmp/btn/bg",
      "$when": {
        "theme": "light",
        "accessibility": "high-contrast"
      },
      "$value": {
        "colorSpace": "srgb",
        "components": [
          0.027450980392156862,
          0.2235294117647059,
          0.4588235294117647
        ]
      }
    },
    {
      "$token": "#/cmp/btn/fg",
      "$when": {
        "theme": "light",
        "accessibility": "high-contrast"
      },
      "$value": {
        "colorSpace": "srgb",
        "components": [
          1,
          1,
          1
        ]
      }
    }
  ]
}
